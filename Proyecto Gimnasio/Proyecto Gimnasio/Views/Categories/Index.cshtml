@model IEnumerable<Proyecto_Gimnasio.Models.Category>

@{
    ViewData["Title"] = "Index";
}
<link rel="stylesheet" href="~/css/category.css" />
<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NameCategory)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="color: white !important;">@item.NameCategory</td>
                <td class="text-center">
                    <a asp-action="Edit" asp-route-id="@item.IdCategory" class="btn btn-warning btn-sm">Editar</a>
                    <a asp-action="Details" asp-route-id="@item.IdCategory" class="btn btn-info btn-sm ms-1">Ver</a>
                    <a asp-action="Delete" asp-route-id="@item.IdCategory" class="btn btn-danger btn-sm ms-1">Eliminar</a>
                </td>
            </tr>
        }
    </tbody>
</table>
